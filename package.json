{
  "name": "@interactjs/_dev",
  "version": "1.9.2",
  "private": true,
  "license": "MIT",
  "workspaces": [
    ".",
    "./@interactjs/*",
    "./interactjs"
  ],
  "directories": {
    "bin": "./bin"
  },
  "scripts": {
    "bootstrap": "npx yarn install --pure-lockfile",
    "start": "_clean && _add_use_modules && cd interactjs && NODE_ENV=development ../bin/_bundle --watch",
    "bundle": "cd interactjs; NODE_ENV=production ../bin/_bundle",
    "esnext": "_add_use_modules && _esnext",
    "build": "_clean && yarn docs && yarn bundle && _add_use_modules && tsc -b -f && _esnext",
    "docs": "node jsdoc/index.js",
    "lint": "_lint --fail-on-error",
    "test": "_test",
    "test:debug": "TEST_RUNNER=nodemon TEST_RUNNER_ARGS='-x node --inspect' yarn test",
    "tsc_lint_test": "_add_use_modules && tsc -b -f && tsc -b --clean && _lint --fail-on-error && _test",
    "dev-link": "yarn link; for p in @interactjs/*; do cd $p; yarn link; cd ../..; done",
    "dev-unlink": "yarn unlink; for p in @interactjs/*; do cd $p; yarn unlink; cd ../..; done"
  },
  "dependencies": {
    "@babel/core": "latest",
    "@babel/plugin-proposal-class-properties": "latest",
    "@babel/plugin-transform-modules-commonjs": "latest",
    "@babel/plugin-transform-runtime": "latest",
    "@babel/preset-env": "latest",
    "@babel/preset-typescript": "latest",
    "@babel/register": "latest",
    "@babel/runtime": "latest",
    "@types/node": "latest",
    "@types/tape": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/eslint-plugin-tslint": "latest",
    "@typescript-eslint/parser": "latest",
    "babel-eslint": "latest",
    "babel-plugin-istanbul": "latest",
    "babel-plugin-transform-inline-environment-variables": "latest",
    "babelify": "latest",
    "browser-pack-flat": "latest",
    "browserify": "latest",
    "combine-source-map": "latest",
    "common-shakeify": "latest",
    "domator": "latest",
    "errorify": "latest",
    "eslint": "latest",
    "eslint-config-standard": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-require-path-exists": "latest",
    "eslint-plugin-standard": "latest",
    "fs-extra": "latest",
    "glob": "latest",
    "husky": "latest",
    "jsdoc": "github:taye/jsdoc#master",
    "jsdoc-babel": "latest",
    "jsdoc-stale": "github:taye/jsdoc-stale#master",
    "jsdom": "latest",
    "lerna": "latest",
    "mkdirp": "latest",
    "nodemon": "latest",
    "nyc": "latest",
    "p-queue": "latest",
    "resolve": "latest",
    "semver": "latest",
    "shelljs": "latest",
    "tap-spec": "latest",
    "tape": "latest",
    "terser": "latest",
    "ts-node": "latest",
    "tslint": "latest",
    "typescript": "3.6",
    "uglify-js": "latest",
    "watchify": "latest",
    "yargs": "latest",
    "yarn": "latest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint --fail-on-error",
      "pre-push": "yarn tsc_lint_test"
    }
  }
}

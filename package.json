{
  "name": "@interactjs/_dev",
  "version": "1.9.9",
  "private": true,
  "license": "MIT",
  "workspaces": [
    ".",
    "./@interactjs/*",
    "./interactjs"
  ],
  "directories": {
    "bin": "./bin"
  },
  "scripts": {
    "bootstrap": "npx yarn install --pure-lockfile",
    "start": "yarn esnext --serve",
    "bundle": "NODE_ENV=production bin/_bundle",
    "esnext": "_add_use_modules && _esnext",
    "build": "_clean && yarn docs && yarn bundle && _add_use_modules && tsc -b -f && _esnext",
    "docs": "node jsdoc/index.js",
    "lint": "_lint --fail-on-error",
    "test": "_test",
    "test:debug": "TEST_RUNNER=nodemon TEST_RUNNER_ARGS='-x node --inspect' yarn test",
    "tsc_lint_test": "_add_use_modules && tsc -b -f && tsc -b --clean && _lint --fail-on-error && _test",
    "dev-link": "yarn link; for p in @interactjs/*; do cd $p; yarn link; cd ../..; done",
    "dev-unlink": "yarn unlink; for p in @interactjs/*; do cd $p; yarn unlink; cd ../..; done"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@types/node": "^13.11.1",
    "@types/shelljs": "^0.8.7",
    "@types/tape": "^4.2.34",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babelify": "^10.0.0",
    "browser-pack-flat": "^3.4.2",
    "browser-sync": "^2.26.7",
    "browserify": "^16.5.1",
    "chokidar": "^3.3.1",
    "combine-source-map": "^0.8.0",
    "common-shakeify": "^0.6.2",
    "domator": "^2.0.0",
    "errorify": "^0.3.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-require-path-exists": "^1.1.9",
    "eslint-plugin-standard": "^4.0.1",
    "fs-extra": "^9.0.0",
    "glob": "^7.1.6",
    "husky": "^4.2.5",
    "jsdoc": "github:taye/jsdoc#master",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-stale": "github:taye/jsdoc-stale#master",
    "jsdom": "^16.2.2",
    "lerna": "^3.20.2",
    "mkdirp": "^1.0.4",
    "nodemon": "^2.0.3",
    "nyc": "^15.0.1",
    "p-queue": "^6.3.0",
    "resolve": "^1.15.1",
    "semver": "^7.3.2",
    "shelljs": "^0.8.3",
    "tap-spec": "^5.0.0",
    "tape": "^4.13.2",
    "temp": "^0.9.1",
    "terser": "^4.6.11",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "typescript": "3.6",
    "uglify-js": "^3.9.1",
    "watchify": "^3.11.1",
    "yargs": "^15.3.1",
    "yarn": "^1.22.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint --fail-on-error",
      "pre-push": "yarn tsc_lint_test"
    }
  }
}

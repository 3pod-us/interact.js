{
  "name": "@interactjs/_dev",
  "version": "1.9.10",
  "private": true,
  "license": "MIT",
  "workspaces": [
    ".",
    "./@interactjs/*",
    "./interactjs"
  ],
  "directories": {
    "bin": "./bin"
  },
  "scripts": {
    "bootstrap": "npx yarn install --pure-lockfile --prefer-offline",
    "start": "yarn esnext --serve",
    "bundle": "NODE_ENV=production _bundle",
    "esnext": "_add_plugin_indexes && _esnext",
    "build": "yarn docs && yarn bundle && _add_plugin_indexes && tsc -b -f && _esnext",
    "docs": "node jsdoc/index.js",
    "lint": "_lint --fail-on-error",
    "test": "_test",
    "test:debug": "TEST_RUNNER=nodemon TEST_RUNNER_ARGS='-x node --inspect' yarn test",
    "tsc_lint_test": "_add_plugin_indexes && tsc -b -f && tsc -b --clean && _lint --fail-on-error && _test",
    "dev-link": "yarn link; for p in @interactjs/*; do yarn --cwd $p link; done",
    "dev-unlink": "yarn unlink; for p in @interactjs/*; do yarn --cwd $p unlink; done"
  },
  "dependencies": {
    "@babel/core": "latest",
    "@babel/plugin-proposal-class-properties": "latest",
    "@babel/plugin-proposal-optional-catch-binding": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "latest",
    "@babel/plugin-transform-runtime": "latest",
    "@babel/preset-env": "latest",
    "@babel/preset-typescript": "latest",
    "@babel/register": "latest",
    "@babel/runtime": "latest",
    "@types/node": "latest",
    "@types/shelljs": "latest",
    "@types/tape": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/eslint-plugin-tslint": "latest",
    "@typescript-eslint/parser": "latest",
    "babel-eslint": "latest",
    "babel-plugin-istanbul": "latest",
    "babelify": "latest",
    "browser-pack-flat": "latest",
    "browser-sync": "latest",
    "browserify": "latest",
    "chokidar": "latest",
    "combine-source-map": "latest",
    "domator": "latest",
    "errorify": "latest",
    "eslint": "latest",
    "eslint-config-standard": "latest",
    "eslint-import-resolver-typescript": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-require-path-exists": "latest",
    "eslint-plugin-standard": "latest",
    "fs-extra": "latest",
    "glob": "latest",
    "husky": "latest",
    "jsdoc": "github:taye/jsdoc#master",
    "jsdoc-babel": "latest",
    "jsdoc-stale": "github:taye/jsdoc-stale#master",
    "jsdom": "latest",
    "lerna": "latest",
    "mkdirp": "latest",
    "nodemon": "latest",
    "nyc": "latest",
    "p-queue": "latest",
    "resolve": "latest",
    "semver": "latest",
    "shelljs": "latest",
    "tap-spec": "latest",
    "tape": "latest",
    "temp": "latest",
    "terser": "latest",
    "ts-node": "latest",
    "tslint": "latest",
    "typescript": "latest",
    "watchify": "latest",
    "yargs": "latest",
    "yarn": "latest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint --fail-on-error",
      "pre-push": "yarn tsc_lint_test"
    }
  }
}
